@model CartonBuilder.Web.ViewModels.CartonIndexViewModel

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Carton Number
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.Cartons)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CartonNumber)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id }) |
                @Html.ActionLink("AddEquipment", "AddEquipment", new { id = item.Id }) |
                @Html.ActionLink("ViewCartonEquipment", "ViewCartonEquipment", new { id = item.Id })
            </td>
        </tr>
    }

</table>
